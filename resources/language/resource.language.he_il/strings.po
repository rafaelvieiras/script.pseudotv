msgctxt "Addon Summary"
msgid "Channel surfing for your video library"
msgstr "גלישה בערוץ עבור ספריית הווידאו שלך"

msgctxt "Addon Description"
msgid "PseudoTV acts similar to normal broadcast or cable TV, complete with multiple preset and user-defined channels and an EPG."
msgstr "פעולות PseudoTV דומות לשידור רגיל או לטלוויזיה בכבלים, עם ערוצים מוגדרים מראש מוגדרים מראש ומדריך שידורים."

msgctxt "#30000"
msgid "General"
msgstr "כללי"

msgctxt "#30001"
msgid "Auto-off Timer"
msgstr "כיבוי על ידי שעון עצר"

msgctxt "#30002"
msgid "Force channel reset on next start"
msgstr "כפה איפוס ערוץ באיתחול הבא"

msgctxt "#30003"
msgid "Show info when changing channels"
msgstr "הצג מידע בעת החלפת ערוצים"

msgctxt "#30004"
msgid "Time between channel resets"
msgstr "פרק הזמן בין ערוץ איפוס"

msgctxt "#30005"
msgid "Coming Up Next"
msgstr "השידור הבא"

msgctxt "#30006"
msgid "Always show channel watermark"
msgstr "הצג תמיד את סימול הערוץ"

msgctxt "#30007"
msgid "Show channel logo in epg grid"
msgstr "הצג סמל ערוץ ברשת מדריך השידורים"

msgctxt "#30008"
msgid "Open Channel Configuration"
msgstr "פתח את הגדרות הערוץ"

# empty string with id 30009
msgctxt "#30010"
msgid "Channel Logo Folder"
msgstr "תיקיית סמלי הערוץ"

msgctxt "#30011"
msgid "Default channel times at startup"
msgstr "ברירת מחדל לזמני ערוץ באיתחול"

msgctxt "#30012"
msgid "Background Updating"
msgstr "עדכון ברקע"

msgctxt "#30013"
msgid "Shared Channels Folder"
msgstr "תיקיית ערוצים משותפים"

msgctxt "#30014"
msgid "Clock Display"
msgstr "תצוגת שעון"

msgctxt "#30015"
msgid "Enable Channel Sharing"
msgstr "אפשר שיתוף ערוץ"

msgctxt "#30016"
msgid "Show Coming Up Next box"
msgstr "הצג תיבת השידור הבא"

msgctxt "#30017"
msgid "Hide very short videos"
msgstr "הסתר קטעי וידאו קצרים מאוד"

msgctxt "#30018"
msgid "Duration of \"short\" videos"
msgstr "משך הזמן של סרטוני וידאו קצרים"

msgctxt "#30019"
msgid "Would you like to save your changes?"
msgstr "האם ברצונך לשמור את השינויים?"

msgctxt "#30020"
msgid "Visuals"
msgstr "חזותי"

msgctxt "#30021"
msgid "Show Season/Episode number"
msgstr "הצג מספר עונה או פרק"

msgctxt "#30022"
msgid "Tweaks"
msgstr "שינויים"

# empty string with id 30023
msgctxt "#30024"
msgid "Background Loading..."
msgstr "טעינה ברקע..."

msgctxt "#30025"
msgid "Added"
msgstr "נוסף"

msgctxt "#30026"
msgid "Auto Start"
msgstr "הפעלה אוטומטית"

msgctxt "#30027"
msgid "Activate Service"
msgstr "הפעל את השירות"

msgctxt "#30028"
msgid "Service Delay"
msgstr "השהיית שירות"

msgctxt "#30029"
msgid "Show Notification"
msgstr "הצג הודעה"

msgctxt "#30030"
msgid "Service Starting..."
msgstr "השירות מתחיל..."

msgctxt "#30031"
msgid "Are you sure you want to exit PseudoTV?"
msgstr "האם אתה בטוח שברצונך לצאת מ-PseudoTV?"

msgctxt "#30032"
msgid "Do you want to save all changes?"
msgstr "האם ברצונך לשמור את כל השינויים?"

msgctxt "#30033"
msgid "Are you sure you want to clear this channel?"
msgstr "האם אתה בטוח שברצונך לאפס את הערוץ הזה?"

msgctxt "#30034"
msgid "Unable to open the cache file"
msgstr "אין אפשרות לפתוח את קובץ המטמון"

msgctxt "#30035"
msgid "Unable to create the cache directory"
msgstr "אין אפשרות ליצור את ספריית מטמון"

msgctxt "#30036"
msgid "Unable to create the storage directory"
msgstr "אין אפשרות ליצור את ספריית אחסון"

msgctxt "#30037"
msgid "Unable to find any channels. Please configure the addon."
msgstr "אין אפשרות לאתר ערוצים. אנא הגדר ההרחבה."

msgctxt "#30038"
msgid "Unable to populate channels. Please verify that you have scraped media in your library and that you have properly configured channels."
msgstr "לא ניתן לאכלס ערוצים. נא ודא כי יש לך מדיה תואמת בספריה שלך ושהגדרת כראוי את הערוצים."

msgctxt "#30039"
msgid "Exceeded 3 invalidated channels. Exiting."
msgstr "חריגה מ 3 ערוצים לא חוקיים. יציאה."

msgctxt "#30040"
msgid "No channels available. Exiting."
msgstr "אין ערוצים זמינים. יציאה."

msgctxt "#30041"
msgid "Coming Up"
msgstr "השידור הבא"

msgctxt "#30042"
msgid "Already Seen"
msgstr "כבר נצפה"

msgctxt "#30043"
msgid "Now Playing"
msgstr "מנגן כעת"

msgctxt "#30044"
msgid "Playlist Media Limit"
msgstr "מגבלת רשימת ניגון מדיה"

msgctxt "#30045"
msgid "OSD Channel Number Colour"
msgstr "צבע מספר ערוץ הוידאו OSD"

msgctxt "#30046"
msgid "Seek step forward"
msgstr "מחפש צעד קדימה"

msgctxt "#30047"
msgid "Seek step backward"
msgstr "מחפש צעד אחורה"

msgctxt "#30190"
msgid "Reset Watched Status"
msgstr "אפס סטטוס נצפה"
